#include <stdio.h>

int main(void) {
  float totaltotal, totalnumero;
  float numero = 0;
  
  while(numero >= 0){
  printf("Digite a nota\n");
  scanf("%f", &numero);
  if(numero >= 0){
  totaltotal += numero;
  totalnumero++;
  }}
  
  numero = totaltotal / totalnumero;
  printf("Sua média é %1.2f", numero);
  
}
